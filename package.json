{
  "name": "shiorif",
  "version": "3.0.2",
  "description": "The convenient SHIORI Shared Library Interface",
  "keywords": [
    "ikagaka",
    "ukagaka",
    "shiori"
  ],
  "main": "./dist/lib/shiorif.js",
  "types": "./dist/lib/shiorif.d.ts",
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "prepare": "npm run dist",
    "prepublishOnly": "npm run lint && npm run test",
    "dist": "npm run node",
    "node": "shx rm -rf dist && cross-env NODE_ENV=production tsc",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "test": "npm run test-node",
    "test-node": "cross-env NODE_ENV=test nyc mocha",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode file --out doc --readme Readme.md --disableOutputCheck lib && cd doc && git status && cd .."
  },
  "dependencies": {
    "shiori_converter": "^2.0.2",
    "shiori_transaction": "^2.0.4",
    "shioriloader": "^2.0.1"
  },
  "peerDependencies": {
    "shiorijk": "^1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^13.1.0",
    "@types/power-assert": "^1.4.29",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "espower-typescript": "^9.0.0",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.19.1",
    "shiorijk": "^1.0.0",
    "shx": "^0.3.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.16.0",
    "typescript": "^3.7.2"
  },
  "url": "https://github.com/Ikagaka/shiorif",
  "author": "Narazaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ikagaka/shiorif.git"
  }
}
